{
  "name": "reprise",
  "version": "0.0.0",
  "description": "Automatic hot-reloading for Babel-compiled Node.js processes",
  "keywords": [
    "hot reloading",
    "reloading",
    "babel"
  ],
  "homepage": "https://github.com/splayd/reprise",
  "bugs": "https://github.com/splayd/reprise/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "bin": {
    "reprise": "bin/reprise.js"
  },
  "repository": "splayd/reprise",
  "scripts": {
    "prepare": "flow-typed update --skip",
    "test": "standard -v --fix && flow && ava -v",
    "release": "semantic-release",
    "build": "build-esm",
    "prepack": "build-esm"
  },
  "dependencies": {
    "chokidar": "^2.0.4",
    "interop-require": "^1.0.0",
    "invalidate-module": "^1.0.0",
    "overdub": "^1.2.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.8",
    "build-esm": "^4.1.0",
    "flow-bin": "^0.80.0",
    "flow-typed": "^2.5.1",
    "fs-extra": "^7.0.0",
    "semantic-release": "^15.9.14",
    "standard-esnext": "^5.1.7",
    "tempy": "^0.2.1"
  },
  "ava": {
    "require": "overdub/register",
    "babel": {
      "testOptions": {
        "extends": "overdub/babel"
      }
    }
  }
}
